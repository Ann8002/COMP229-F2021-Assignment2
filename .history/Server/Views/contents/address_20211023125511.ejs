<div class="p-3 mb-2 bg-info text-center"><h1>Business Contact List</h1>
</div>


<table class="table table-bordered">
    <tr>
        <th class="text-center">Name</th>
        <th class="text-center">Contact Number</th>
        <th class="text-center">Email Address</th>
    </tr>

    <!-- Repeatable Template Row -->

    <% for(let count = 0; count < AddressList.length; count++) {  %>
        <tr>
            <td class="text-center"><%= AddressList[count].name %></td>  
            <td class="text-center"><%= AddressList[count].number %></td> 
            <td class="text-center"><%= AddressList[count].email %></td> 
        </tr>
    <% } %>
  </table>